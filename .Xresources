! Colors
URxvt*background:  [90]#030609
URxvt*tintColor:   #030609
URxvt*foreground:  #8fb3d8
URxvt*borderColor: #8fb3d8
!black
URxvt*color0:      #0c141f
URxvt*color8:      #0c141f
!red
URxvt*color1:      #ffe64d
URxvt*color9:      #df740c
!green
URxvt*color2:      #9bbadc
URxvt*color10:     #9bbadc
!yellow
URxvt*color3:      #df740c
URxvt*color11:     #ffe64d
!blue
URxvt*color4:      #9bbadc
URxvt*color12:     #9bbadc
!magenta
URxvt*color5:      #df740c
URxvt*color13:     #df740c
!cyan
URxvt*color6:      #9bbadc
URxvt*color14:     #9bbadc
!white
URxvt*color7:      #8fb3d8
URxvt*color15:     #8fb3d8

! Xft
Xft*dpi:        96
Xft*antialias:  true
Xft*rgba:       rgb
Xft*hinting:    true

! URxvt
URxvt*depth:            32
URxvt*geometry:         90x30
URxvt*transparent:      true
URxvt*font:             xft:Inconsolata for Powerline:size=10
URxvt*scrollBar:        false
URxvt*internalBorder:   5
URxvt*lineSpace:        0
URxvt*loginShell:       true
URxvt*cursorBlink:      true
URxvt*cursorUnderline:  true

